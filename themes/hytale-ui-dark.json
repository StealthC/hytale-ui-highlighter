{
  "name": "Hytale UI Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorCursor.foreground": "#aeafad",
    "editor.lineHighlightBackground": "#2d2d30",
    "editorLineNumber.foreground": "#6a9955"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment.line.hytale-ui",
      "settings": {
        "foreground": "#6a9955",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Spread Operator",
      "scope": "keyword.operator.spread.hytale-ui",
      "settings": {
        "foreground": "#d19a66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable ($)",
      "scope": "variable.other.hytale-ui",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Function Call",
      "scope": "entity.name.function.hytale-ui",
      "settings": {
        "foreground": "#dcdcaa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Template (@)",
      "scope": "entity.name.selector.hytale-ui",
      "settings": {
        "foreground": "#ce9178",
        "fontStyle": "bold"
      }
    },
    {
      "name": "ID (#)",
      "scope": "entity.other.attribute-name.id.hytale-ui",
      "settings": {
        "foreground": "#f48771",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Component Name",
      "scope": "string.unquoted.hytale-ui",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "I18n Key",
      "scope": "variable.language.hytale-ui",
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "name": "String",
      "scope": "string.quoted.double.hytale-ui",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Escape Character",
      "scope": "constant.character.escape.hytale-ui",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric.hytale-ui",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Hex Color",
      "scope": "constant.numeric.hex.hytale-ui",
      "settings": {
        "foreground": "#ce9178",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.hytale-ui",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword Control",
      "scope": "keyword.control.hytale-ui",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Colon",
      "scope": "punctuation.separator.key-value.hytale-ui",
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Accessor Dot",
      "scope": "punctuation.separator.accessor.hytale-ui",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator.comma.hytale-ui",
        "punctuation.section.braces.begin.hytale-ui",
        "punctuation.section.braces.end.hytale-ui",
        "punctuation.section.parens.begin.hytale-ui",
        "punctuation.section.parens.end.hytale-ui",
        "punctuation.section.brackets.begin.hytale-ui",
        "punctuation.section.brackets.end.hytale-ui"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    }
  ]
}
